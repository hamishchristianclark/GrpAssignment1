<?xml version="1.0" ?>
 
<sdf version="1.5">
	<world name="default">
		<!-- A global light source -->
		<include>
			<uri>model://sun</uri>
		</include>
 
		<!-- A ground plane -->
		<include>
			<uri>model://ground_plane</uri>
		</include>

		<!-- kinect -->
		<include>
			<uri>model://kinect_ros</uri>
			<pose> 1.0 1.0 3.0 0.0 1.58 0.0 </pose> <?x y z roll pitch yaw?>
            <static> 1 </static>
		</include>         
	</world>
</sdf>
